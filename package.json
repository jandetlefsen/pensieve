{
  "name": "boreas",
  "version": "0.0.0",
  "description": "memory tool",
  "main": "src/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run start:prod || yarn run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base public/ --history-api-fallback",
    "start:prod": "yarn run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "yarn run build:client && yarn run build:server",
    "build:old": "babel src -d build",
    "webpack": "webpack-dev-server --progress --color",
    "start:old": "nodemon server/index.js --exec babel-node",
    "start:olddev": "babel-node server/index.js",
    "dev": "npm run start && webpack-dev-server --progress --color",
    "db": "mongo boreas",
    "db:seed": "babel-node src/db/seeder.js",
    "email:test": "babel-node src/emails/test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlaz/boreas.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nlaz/boreas/issues"
  },
  "homepage": "https://github.com/nlaz/boreas#readme",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.5",
    "connect-flash": "^0.1.1",
    "cron": "^1.2.1",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "if-env": "^1.0.0",
    "moment": "^2.17.1",
    "mongodb": "^2.2.19",
    "mongoose": "4.3.3",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "redux-thunk": "^2.2.0",
    "sendgrid": "^4.7.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "jsonwebtoken": "^7.3.0",
    "nodemon": "^1.11.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2"
  }
}
